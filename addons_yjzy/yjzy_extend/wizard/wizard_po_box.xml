<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="wizard_po_box_form" model="ir.ui.view">
            <field name="name">wizard.po.box.from</field>
            <field name="model">wizard.po.box</field>
            <field name="arch" type="xml">
                <form string="分配箱号">
                    <group><group>
                        <field name="box_type"/>
                        <field name="box_start" attrs="{'invisible': [('box_type','=','a')]}"/>
                    </group>
                    </group>
                    <footer>
                        <button name="apply" class="oe_highlight" type='object' string="确认"/>
                        <button special="cancel" class="oe_highlight" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>

