                                                </group>

                                            </page>

                                        </notebook>


                                    </form>
                                </field>
                                <field name="description_baoguan"/>

                            </page>
                            <page string="明细批次" attrs="{'readonly': [('locked','=',True)]}" name="pcmx" groups="base.group_system">
