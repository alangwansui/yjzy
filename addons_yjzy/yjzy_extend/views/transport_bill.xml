                                    </tree>
                                </field>
                            </page>

                            <page string="报关金额">
                                <field name="pack_line_ids2">
                                    <tree editable="top">
                                        <field name="sale_currency_id" invisible="1"/>
                                        <field name="company_currency_id" invisible="1"/>
                                        <field name="hs_code"/>
