<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="excel_text_parser_form" model="ir.ui.view">
        <field name="name">excel.text.parser.view.form</field>
        <field name="model">excel.text.parser</field>
        <field name="arch" type="xml">
            <form string="Excel复制内容">
                <group>

                    <field name="content"/>
                </group>
                <footer>
                    <button name="apply" string="确认" type="object"/>
                    <button name="clear" type="object" string="清空"/>
                    <button string="取消" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>

